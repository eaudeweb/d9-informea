        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "4.5.0",
        "drupal/paragraphs": "1.12.0",
        "drupal/pathauto": "1.8.0",
        "drupal/powertagging": "1.1.0",
        "drupal/pp_taxonomy_manager": "1.0.0",
        "drupal/purge": "dev-3.x",
        "drupal/raven": "2.26.0",
        "drupal/recaptcha": "3.0.0",
        "drupal/redirect": "1.6.0",
        "drupal/remove_username": "1.0.0",
        "drupal/search_api": "^1.19",
        "drupal/search_api_attachments": "1.0.0-beta16",
        "drupal/search_api_solr": "4.1.1",
        "drupal/search_api_sorts": "1.0.0-beta1",
