        "drupal/better_exposed_filters": "5.0.0-beta1",
        "drupal/block_field": "1.0.0-rc1",
        "drupal/bootstrap_barrio": "4.28.0",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/captcha": "1.1.0",
        "drupal/chosen": "2.9.0",
        "drupal/ckeditor_bootstrap_grid": "1.3.0",
        "drupal/colorbox": "1.6.0",
        "drupal/core-composer-scaffold": "8.9.11",
        "drupal/core-recommended": "^8.9.11",
