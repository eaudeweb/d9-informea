        "cweagans/composer-patches": "1.7.0",
        "datatables/datatables": "1.10.19",
        "desandro/imagesloaded": "3.1.8",
        "desandro/masonry": "3.3.1",
        "drupal/admin_toolbar": "2.2.0",
