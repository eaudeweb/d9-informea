{
    "name": "unep/leap",
    "description": "UNEP LEAP project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Eau de Web"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        "colorbox": {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "1.6.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
                    "type": "zip"
                }
            }
        },
        "masonry": {
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "3.3.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v3.3.1.zip",
                    "type": "zip"
                }
            }
        },
        "imagesloaded": {
            "type": "package",
            "package": {
                "name": "desandro/imagesloaded",
                "version": "3.1.8",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/imagesloaded/archive/v3.1.8.zip",
                    "type": "zip"
                }
            }
        },
        "dropzone": {
            "type": "package",
            "package": {
                "name": "enyoo/dropzone",
                "version": "4.3.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
                    "type": "zip"
                }
            }
        },
        "datatables": {
            "type": "package",
            "package": {
                "name": "datatables/datatables",
                "version": "1.10.19",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/DataTables/DataTables/archive/1.10.19.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    },
    "require": {
