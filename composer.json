        "drupal/varnish_purge": "2.0.0",
        "drupal/video_embed_field": "2.4.0",
        "drupal/video_embed_html5": "1.2.0",
        "drupal/views_data_export": "^1.0",
        "drupal/webform": "^6.0",
        "drupal/xls_serialization": "1.2.0",
        "drupal/xmlsitemap": "1.0.0-rc1",
        "drush/drush": "^10.3",
        "eaudeweb/heavy-lifter": "^1.3.8",
        "enyoo/dropzone": "4.3.0",
        "harvesthq/chosen": "1.8.7",
        "jackmoore/colorbox": "1.6.4",
        "oomphinc/composer-installers-extender": "^2.0",
        "phpmailer/phpmailer": "6.1.4",
        "symfony/event-dispatcher": "3.4.41",
        "vlucas/phpdotenv": "2.6.1",
