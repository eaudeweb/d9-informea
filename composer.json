        "drupal/editor_file": "1.5.0",
        "drupal/edw_healthcheck": "1.8.0",
        "drupal/entity_reference_revisions": "1.8.0",
        "drupal/facets": "dev-1.x",
        "drupal/features": "3.11.0",
        "drupal/field_group": "3.1.0",
        "drupal/file_browser": "1.3.0",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^2.0",
        "drupal/image_widget_crop": "2.3.0",
        "drupal/imce": "2.3.0",
        "drupal/lang_dropdown": "^2.0@beta",
        "drupal/languagefield": "1.5.0",
        "drupal/mail_login": "^2.4",
        "drupal/masquerade": "2.0.0-beta4",
        "drupal/menu_block": "1.6.0",
