{% macro get(icon, theme_path, width, height, class) %}
{% spaceless %}
  <svg class="icon icon-{{ icon|clean_class }} {{ class|default('') }}" width="{{ width|default(32) }}" height={{ height|default(32) }} aria-hidden="true">
    <use xlink:href={{ theme_path ~ '/assets/symbol/svg/sprite.symbol.svg#' ~  icon }} ></use>
  </svg>
{% endspaceless %}
{% endmacro %}
